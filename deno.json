{
  "imports": {
    "@planetscale/database": "https://esm.sh/*@planetscale/database@1.4.0"
  },
  "tasks": {
    "start": "deno run  --unstable --allow-net --allow-env src/index.ts",
    "dev": "deno run  --unstable --allow-net --allow-env --watch=src src/index.ts"
  },
  "fmt": {
    "files": {
      "include": [
        "src/"
      ]
    },
    "options": {
      "useTabs": true,
      "lineWidth": 120,
      "indentWidth": 2,
      "singleQuote": true,
      "proseWrap": "preserve"
    }
  },
  "lint": {
    "files": {
      "include": [
        "src/"
      ]
    }
  }
}